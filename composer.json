{
  "name": "pxl8/wordpress-plugin",
  "description": "WordPress plugin for PXL8 image optimization and delivery",
  "type": "wordpress-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "PXL8 Team",
      "email": "support@pxl8.ru"
    }
  ],
  "repositories": [
    {
      "type": "path",
      "url": "../pxl8-sdk-php"
    }
  ],
  "require": {
    "php": ">=7.4",
    "pxl8/sdk-php": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Pxl8\\WordPress\\": "includes/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
